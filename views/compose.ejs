<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
     <h3>Title</h3>
     <textarea class = "form-control" name="postTitle" rows="1" cols="50"></textarea>
  </div>
  <div class="form-group">
    <h3>Content</h3>
    <textarea class = "form-control content-box" name="postContent" rows="10" cols="50"></textarea>
    <button class="btn btn-primary" type="submit" name="button">Upload</button>
  </div>
</form>
<%- include("partials/footer") -%>
