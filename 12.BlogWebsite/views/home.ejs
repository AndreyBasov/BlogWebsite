<%- include("partials/header") -%>
<h1>Home</h1>
<p><%=startingContent %></p>
<% posts.forEach((element) =>  {%>
<div class="form-group">
  <h1><%= element.postTitle%></h1>
  <p><%= element.postContent.slice(0, 250)%>...</p>
  <a href="/posts/+ <%= element.postTitle%>">Read more</a>
</div>
<% }); %>
<%- include("partials/footer") -%>
